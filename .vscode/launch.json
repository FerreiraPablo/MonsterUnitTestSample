{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/MonsterUnitTestSample.Application/bin/Debug/net6.0/MonsterUnitTestSample.Application.dll",
            "args": [],
            "cwd": "${workspaceFolder}/MonsterUnitTestSample.Application",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": ".NET Core Test",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "dotnet",
            "args": ["test", "${workspaceFolder}/MonsterUnitTestSample.Tests/MonsterUnitTestSample.Tests.csproj"],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}